{
  "hosting": [{
    "cleanUrls": true,
    "target": "assets",
    "public": "dist",
    "headers": [{
      "source" : "**",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "no-store, max-age=0"
      },
      {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
    }]
  }]
}
